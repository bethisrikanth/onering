[{
  "_id": "default",
  "items": [{
    "type": "image",
    "target": "/"
  }, {
    "type": "menu",
    "name": "Infrastructure",
    "icon": "cog",
    "items": [{
      "name": "Overview",
      "href": "#/overview",
      "icon": "globe"
    },{
      "type":  "query",
      "query": "/api/devices/summary/by-site",
      "template": {
        "name":    "{{ field.id | uppercase }}",
        "counter": "{{ field.count }}",
        "href":    "#/site/{{ field.id }}"
      }
    }, {
      "type": "menu",
      "name": "Status",
      "icon": "check",
      "items": [{
        "type":  "query",
        "query": "/api/devices/summary/by-status",
        "template": {
          "name":    "{{ field.id | titleize }}",
          "icon":    "status-{{ field.id }}",
          "counter": "{{ field.count }}",
          "href":    "#/inf/show/status/{{ field.id }}"
        }
      }]
    }]
  }]
}]