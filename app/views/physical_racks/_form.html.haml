= form_for @physical_rack do |f|
  -if @physical_rack.errors.any?
    #error_explanation
      %h2= "#{pluralize(@physical_rack.errors.count, "error")} prohibited this physical_rack from being saved:"
      %ul
        - @physical_rack.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :index
    = f.number_field :index
  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
